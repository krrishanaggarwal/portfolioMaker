
  <section class="contact" id="contact">
    <div class="content">
      <div class="title"><span>Contact Me</span></div>
      <div class="text">
        <%=about.address%>
        <div class="button">

          <button><span class="iconify" data-icon="bx:phone-call"></span><%=about.contact%></button>
        </div>
        <div class="button"> 
          <button><span class="iconify-inline" data-icon="ant-design:mail-filled"></span><a class="email" href="mailto:<%=about.email%>"><%=about.email%></a></button>
        </div>
      </div>
    </div>
  </section>